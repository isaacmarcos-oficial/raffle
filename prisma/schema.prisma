generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Ticket {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  number       String
  buyer        String
  phone        String?
  paid         Boolean
  purchaseDate DateTime
}